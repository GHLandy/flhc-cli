{
  "name": "flhc-cli",
  "version": "0.0.1",
  "description": "> flhc-cli",
  "author": "GHLandy <ghlandy@ghlandy.com>",
  "license": "MIT",
  "bin": {
    "flhc": "bin/flhc"
  },
  "files": [
    "bin",
    "lib",
    "index.js"
  ],
  "scripts": {
    "format": "prettier --write \"**/*.{js,md}\"",
    "lint": "eslint --fix \"**/*.js\"",
    "lint-staged": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "common-bin": "^2.8.1",
    "shelljs": "^0.8.3"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.1.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0"
  },
  "lint-staged": {
    "bin/flhc": [
      "prettier --write",
      "git add"
    ],
    "**/*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
